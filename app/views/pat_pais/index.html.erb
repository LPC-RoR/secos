<p id="notice"><%= notice %></p>

<h1>Pat Pais</h1>

<table>
  <thead>
    <tr>
      <th>Patente</th>
      <th>Pais</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pat_pais.each do |pat_pai| %>
      <tr>
        <td><%= pat_pai.patente_id %></td>
        <td><%= pat_pai.pais_id %></td>
        <td><%= link_to 'Show', pat_pai %></td>
        <td><%= link_to 'Edit', edit_pat_pai_path(pat_pai) %></td>
        <td><%= link_to 'Destroy', pat_pai, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pat Pai', new_pat_pai_path %>

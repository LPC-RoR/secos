<p id="notice"><%= notice %></p>

<h1>Ae Is</h1>

<table>
  <thead>
    <tr>
      <th>Articulo entrevista</th>
      <th>Investigador</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ae_is.each do |ae_i| %>
      <tr>
        <td><%= ae_i.articulo_entrevista_id %></td>
        <td><%= ae_i.investigador_id %></td>
        <td><%= link_to 'Show', ae_i %></td>
        <td><%= link_to 'Edit', edit_ae_i_path(ae_i) %></td>
        <td><%= link_to 'Destroy', ae_i, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


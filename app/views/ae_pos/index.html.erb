<p id="notice"><%= notice %></p>

<h1>Ae Pos</h1>

<table>
  <thead>
    <tr>
      <th>Articulo entrevista</th>
      <th>Publico objetivo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ae_pos.each do |ae_po| %>
      <tr>
        <td><%= ae_po.articulo_entrevista_id %></td>
        <td><%= ae_po.publico_objetivo %></td>
        <td><%= link_to 'Show', ae_po %></td>
        <td><%= link_to 'Edit', edit_ae_po_path(ae_po) %></td>
        <td><%= link_to 'Destroy', ae_po, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ae Po', new_ae_po_path %>

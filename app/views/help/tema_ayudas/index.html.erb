<p id="notice"><%= notice %></p>

<h1>Tema Ayudas</h1>

<table>
  <thead>
    <tr>
      <th>Orden</th>
      <th>Tema ayuda</th>
      <th>Detalle</th>
      <th>Tipo</th>
      <th>Ilustracion</th>
      <th>Ilustracion cache</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tema_ayudas.each do |tema_ayuda| %>
      <tr>
        <td><%= tema_ayuda.orden %></td>
        <td><%= tema_ayuda.tema_ayuda %></td>
        <td><%= tema_ayuda.detalle %></td>
        <td><%= tema_ayuda.tipo %></td>
        <td><%= tema_ayuda.ilustracion %></td>
        <td><%= tema_ayuda.ilustracion_cache %></td>
        <td><%= link_to 'Show', tema_ayuda %></td>
        <td><%= link_to 'Edit', edit_tema_ayuda_path(tema_ayuda) %></td>
        <td><%= link_to 'Destroy', tema_ayuda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tema Ayuda', new_tema_ayuda_path %>

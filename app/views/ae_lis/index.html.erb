<p id="notice"><%= notice %></p>

<h1>Ae Lis</h1>

<table>
  <thead>
    <tr>
      <th>Articulo entrevista</th>
      <th>Linea investigacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ae_lis.each do |ae_li| %>
      <tr>
        <td><%= ae_li.articulo_entrevista_id %></td>
        <td><%= ae_li.linea_investigacion_id %></td>
        <td><%= link_to 'Show', ae_li %></td>
        <td><%= link_to 'Edit', edit_ae_li_path(ae_li) %></td>
        <td><%= link_to 'Destroy', ae_li, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

